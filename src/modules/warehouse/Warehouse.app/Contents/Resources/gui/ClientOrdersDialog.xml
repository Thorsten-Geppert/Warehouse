<?xml version="1.0"?>
<resource version="2.3.0.1">

	<object class="wxDialog" name="ClientOrdersDialog">
		<title>Orders</title>

		<object class="wxPanel" name="mainPanel">
			<object class="wxBoxSizer">
				<orient>wxVERTICAL</orient>

				<object class="sizeritem">
					<flag>wxEXPAND</flag>

					<object class="Toolbar" name="mainToolbar">
						<buttons>
							<ToolbarButton name="updateToolbarButton">
								<label>Update</label>
								<bitmap platform="win|unix">resources/images/Update.png</bitmap>
								<bitmap platform="mac">Warehouse.app/Contents/Resources/images/Update.png</bitmap>
							</ToolbarButton>

							<ToolbarButtonStretchSpacer />

							<ToolbarButton name="newToolbarButton">
								<label>New</label>
								<bitmap platform="win|unix">resources/images/New.png</bitmap>
								<bitmap platform="mac">Warehouse.app/Contents/Resources/images/New.png</bitmap>
							</ToolbarButton>

							<ToolbarButton name="editToolbarButton">
								<label>Edit</label>
								<bitmap platform="win|unix">resources/images/Edit.png</bitmap>
								<bitmap platform="mac">Warehouse.app/Contents/Resources/images/Edit.png</bitmap>
							</ToolbarButton>

							<ToolbarButton name="removeToolbarButton">
								<label>Remove</label>
								<bitmap platform="win|unix">resources/images/Remove.png</bitmap>
								<bitmap platform="mac">Warehouse.app/Contents/Resources/images/Remove.png</bitmap>
							</ToolbarButton>

							<ToolbarButtonStretchSpacer />

							<ToolbarButton name="findToolbarButton">
								<label>Find</label>
								<bitmap platform="win|unix">resources/images/Find.png</bitmap>
								<bitmap platform="mac">Warehouse.app/Contents/Resources/images/Find.png</bitmap>
							</ToolbarButton>

							<ToolbarButtonStretchSpacer />

							<ToolbarButton name="exportToolbarButton">
								<label>Export</label>
								<bitmap platform="win|unix">resources/images/Export.png</bitmap>
								<bitmap platform="mac">Warehouse.app/Contents/Resources/images/Export.png</bitmap>
							</ToolbarButton>

							<ToolbarButtonStretchSpacer />

							<ToolbarButton name="useToolbarButton">
								<label>Use</label>
								<bitmap platform="win|unix">resources/images/Use.png</bitmap>
								<bitmap platform="mac">Warehouse.app/Contents/Resources/images/Use.png</bitmap>
							</ToolbarButton>

							<ToolbarButtonStretchSpacer />

							<ToolbarButton name="cancelToolbarButton">
								<label>Cancel</label>
								<bitmap platform="win|unix">resources/images/Cancel.png</bitmap>
								<bitmap platform="mac">Warehouse.app/Contents/Resources/images/Cancel.png</bitmap>
							</ToolbarButton>
						</buttons>
					</object>
				</object>

				<object class="sizeritem">
					<flag>wxEXPAND | wxALL</flag>
					<border>15</border>
					<option>1</option>


					<object class="wxListCtrl" subclass="DynSoft::DSListCtrl" name="ordersListCtrl">
						<style>wxLC_REPORT | wxLC_VRULES | wxBORDER_SUNKEN | wxLC_SINGLE_SEL</style>
					</object>
				</object>

			</object>
		</object>
	</object>

</resource>
